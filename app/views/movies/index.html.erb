

<h1>Directors</h1>

<table class="table table-striped" width="80">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Bio</th>
    <th>Dob</th>
    <th>Image_url</th>
    <th>Created_at</th>
    <th>Updated_at</th>
    <th></th>
    <th></th>

  </tr>
 <% @list_of_directors.each do |director|%>
  <tr>

    <td><%= director.id %></td>
    <td><%= director.name %></td>
    <td><%= director.bio %></td>
    <td><%= director.dob %></td>
    <td><%= director.image_url %></td>
    <td><%= director.created_at %></td>
    <td><%= director.updated_at %></td>
    <td><a href="http://localhost:3000/edit_form/<%= director.id %>">Edit</a></td>
    <td><a href="http://localhost:3000/delete_director/<%= director.id %>">Delete</a></td>
  </tr>
  <% end %>



</table>


<a href="http://localhost:3000/directors/new_form">Create</a>
